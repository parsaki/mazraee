<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/home6.btn_newsave"
        android:layout_width="@dimen/_70sdp"
        android:layout_height="@dimen/_28sdp"
        android:layout_marginTop="@dimen/_6sdp"
        android:layout_marginEnd="@dimen/_6sdp"
        android:background="@drawable/buttonleft"
        android:fontFamily="@font/sahel"
        android:text="@string/import_exel"
        android:textColor="@color/white"
        android:textSize="@dimen/_15sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.06"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent="0.26"
        tools:ignore="TouchTargetSizeCheck" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/home6.btn_applysave"
        android:layout_width="@dimen/_70sdp"
        android:layout_height="@dimen/_28sdp"
        android:layout_marginStart="@dimen/_6sdp"
        android:layout_marginTop="@dimen/_6sdp"
        android:background="@drawable/buttonright"
        android:fontFamily="@font/sahel"
        android:text="@string/save"
        android:textColor="@color/white"
        android:textSize="@dimen/_15sdp"
        app:layout_constraintHeight_percent="0.06"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent="0.26"
        tools:ignore="TouchTargetSizeCheck" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/home6.namekhorak.l"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_54sdp"
        android:layout_marginHorizontal="@dimen/_6sdp"
        android:layout_marginTop="@dimen/_11sdp"
        android:hint="نام خوراک"
        app:boxStrokeWidth="0dp"
        app:boxStrokeWidthFocused="0dp"
        android:textColorHint="@color/calypso"
        app:layout_constraintTop_toBottomOf="@id/home6.btn_applysave">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/home6.namekhorak.text"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/edittext1"
            android:fontFamily="@font/sahel"
            android:gravity="center|start"
            android:paddingHorizontal="@dimen/_14sdp"
            android:textSize="@dimen/_16sdp" />
    </com.google.android.material.textfield.TextInputLayout>

    <TextView
        android:id="@+id/home6.textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="7dp"
        android:layout_marginTop="@dimen/_9sdp"
        android:layout_marginEnd="@dimen/_12sdp"
        android:fontFamily="@font/sahel"
        android:text="نام کنسانتره"
        android:textColor="@color/calypso"
        android:textSize="@dimen/_16sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/home6.namekhorak.l" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/home6.hagm.l"
        android:layout_width="0dp"
        android:layout_height="@dimen/_54sdp"
        android:layout_marginHorizontal="@dimen/_6sdp"
        android:layout_marginStart="@dimen/_12sdp"
        android:layout_marginTop="@dimen/_4sdp"
        android:hint="حجم"
        app:boxStrokeWidth="0dp"
        app:boxStrokeWidthFocused="0dp"
        android:textColorHint="@color/calypso"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/home6.textView2"
        app:layout_constraintWidth_percent="0.29">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/home6.hagm.text"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/edittext1"
            android:fontFamily="@font/sahel"
            android:gravity="center"
            android:paddingHorizontal="@dimen/_14sdp"
            android:textSize="@dimen/_16sdp" />
    </com.google.android.material.textfield.TextInputLayout>

    <Spinner
        android:id="@+id/hom6.spiner"
        android:layout_width="0dp"
        android:layout_height="@dimen/_54sdp"
        android:layout_marginTop="@dimen/_4sdp"
        android:layout_marginHorizontal="@dimen/_6sdp"
        android:background="@drawable/spiner1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/home6.hagm.l"
        app:layout_constraintTop_toBottomOf="@id/home6.textView2" />

    <TextView
        android:id="@+id/home6.textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_4sdp"
        android:layout_marginEnd="@dimen/_10sdp"
        android:fontFamily="@font/sahel"
        android:text="نام خوراک"
        android:textColor="@color/calypso"
        android:textSize="@dimen/_16sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/hom6.spiner" />

    <View
        android:id="@+id/home6.view"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_256sdp"
        android:translationZ="10dp"
        android:layout_marginTop="@dimen/_9sdp"
        android:layout_marginHorizontal="@dimen/_6sdp"
        android:background="@drawable/kharidaranview"
        app:layout_constraintHeight_percent="0.36"
        app:layout_constraintTop_toBottomOf="@id/home6.textView3" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/home6.guid1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.3855" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/home6.guid2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.6658" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/home6.guid3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.0638" />
    <TextView
        android:id="@+id/home6.text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sahel"
        android:textSize="@dimen/_15sdp"
        android:textColor="@color/calypso"
        android:text="قیمت هر کیلو"
        android:layout_marginTop="@dimen/_10sdp"
        android:layout_marginStart="@dimen/_18sdp"
        app:layout_constraintStart_toStartOf="@id/home6.view"
        app:layout_constraintEnd_toEndOf="@id/home6.guid1"
        app:layout_constraintTop_toTopOf="@id/home6.view"/>
    <TextView
        android:id="@+id/home6.text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sahel"
        android:textSize="@dimen/_15sdp"
        android:textColor="@color/calypso"
        android:text="مقدار"
        android:layout_marginTop="@dimen/_10sdp"
        app:layout_constraintStart_toStartOf="@id/home6.guid1"
        app:layout_constraintEnd_toEndOf="@id/home6.guid2"
        app:layout_constraintTop_toTopOf="@id/home6.view"/>
    <TextView
        android:id="@+id/home6.text3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sahel"
        android:textSize="@dimen/_15sdp"
        android:textColor="@color/calypso"
        android:text="خوراکی"
        android:layout_marginTop="@dimen/_10sdp"
        app:layout_constraintStart_toStartOf="@id/home6.guid2"
        app:layout_constraintEnd_toEndOf="@id/home6.view"
        app:layout_constraintTop_toTopOf="@id/home6.view"/>
    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/home6.recycleview"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginHorizontal="@dimen/_3sdp"
        android:background="@drawable/view_recycle"
        android:layout_marginTop="@dimen/_13sdp"
        app:layout_constraintTop_toBottomOf="@id/home6.text1"
        app:layout_constraintBottom_toBottomOf="@id/home6.view"
        app:layout_constraintStart_toStartOf="@id/home6.view"
        app:layout_constraintEnd_toEndOf="@id/home6.view"/>
    <TextView
        android:id="@+id/home6.textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_13sdp"
        android:layout_marginEnd="@dimen/_26sdp"
        android:fontFamily="@font/sahel"
        android:text="مقدار کل خوراک :"
        android:textColor="@color/calypso"
        android:textSize="@dimen/_14sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/home6.view" />
    <TextView
        android:id="@+id/home6.textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_14sdp"
        android:layout_marginEnd="@dimen/_26sdp"
        android:fontFamily="@font/sahel"
        android:text="قیمت کل خوراک :"
        android:textColor="@color/calypso"
        android:textSize="@dimen/_14sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/home6.textView4" />
    <EditText
        android:id="@+id/home6.edittext1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:textColor="@color/white"
        android:textSize="@dimen/_14sdp"
        android:fontFamily="@font/sahel"
        android:gravity="center"
        android:editable="false"
        android:background="@drawable/edittextblue"
        app:layout_constraintStart_toStartOf="@id/home6.guid3"
        app:layout_constraintTop_toTopOf="@id/home6.textView4"
        app:layout_constraintBottom_toBottomOf="@id/home6.textView4"
        app:layout_constraintWidth_percent="0.4158"/>
    <EditText
        android:id="@+id/home6.edittext2"
        android:layout_width="0dp"        android:layout_height="0dp"
        android:textColor="@color/white"
        android:textSize="@dimen/_14sdp"
        android:fontFamily="@font/sahel"
        android:gravity="center"
        android:editable="false"
        android:background="@drawable/edittextblue"
        app:layout_constraintStart_toStartOf="@id/home6.guid3"
        app:layout_constraintTop_toTopOf="@id/home6.textView5"
        app:layout_constraintBottom_toBottomOf="@id/home6.textView5"
        app:layout_constraintWidth_percent="0.4158"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/_20sdp"
        app:layout_constraintTop_toBottomOf="@id/home6.textView5"
        app:layout_constraintBottom_toBottomOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>